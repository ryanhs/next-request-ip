    "test:e2e": "jest test/e2e"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsdown": "^0.18.1",
    "typescript": "^5.9.3"
