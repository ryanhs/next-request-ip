{
  "name": "next-request-ip",
  "type": "module",
  "version": "1.0.2",
  "private": false,
  "engines": {
    "node": ">=20"
  },
  "license": "MIT",
  "description": "A Next.js-friendly fork of request-ip, optimized for Headers to retrieve client IP addresses.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanhs/next-request-ip.git"
  },
  "author": "Ryan Hamonangan - mr.ryansilalahi@gmail.com",
  "keywords": [
    "next",
    "nextjs",
    "request-ip",
    "ip",
